{
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "dest": "../public",
    "globalMetadata": {
      "_appTitle": "Arinc424.NET"
    },
    "template": [
      "default",
      "modern",
      "templates/custom"
    ]
  },
  "metadata": [
    {
      "filter": "filter.yml",
      "dest": "api/",
      "enumSortOrder": "declaringOrder",
      "namespaceLayout": "nested",
      "src": [
        {
          "files": [
            "source/Arinc424.csproj"
          ],
          "src": "../"
        }
      ]
    }
  ]
}
